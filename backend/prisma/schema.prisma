
generator dart {
  provider = "dart generator/generator.dart"
  output   = "../lib"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  email       String   @unique
  name        String
  imageUrl    String?
  active      Boolean  @default(true)
  password    String
  role        Role     @default(dev)
}

enum Role {
  dev
  manager
  admin
}